#=================================================================
test: print, x, and set
#=================================================================
target: -c {TG_INSTALL}/fsim-configs/dvfs-default.cfg,{TG_INSTALL}/fsim-configs/localhost.cfg,rtl-asm.cfg -f {TG_INSTALL}/bin/fsim-rtlsim -w {TG_INSTALL}/../fsim/tests/rtl-asm
file: {TG_INSTALL}/../fsim/tests/rtl-asm/rtl-asm

in: break *0x30060
ignore-output
in: run
ignore-output

in: print $r12
out: \$1 = 16672

in: print/x $r12
out: \$2 = 0x4120

in: print/a $pc
out: \$3 = 0x30060 <__start\+96>

in: x/i 0x30000
out: 0x30000 <__start>:\s+movimm\s+r10,16420,64

in: x/i $pc
out: => 0x30060 <__start\+96>:\s+store\s+r2,r4,r5,64

in: set $r1=42
#No output

in: print $r1
out: \$4 = 42
